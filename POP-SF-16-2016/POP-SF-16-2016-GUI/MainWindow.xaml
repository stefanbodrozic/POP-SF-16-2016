<Window x:Class="POP_SF_16_2016_GUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:POP_SF_16_2016_GUI"
        mc:Ignorable="d"
        Title="GLAVNI PROZOR" Height="350" Width="525">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="51*"/>
            <RowDefinition Height="13*"/>
        </Grid.RowDefinitions>
        <ListBox Grid.Row="0" x:Name="lbNamestaj" HorizontalAlignment="Left" Height="350" VerticalAlignment="Top" Width="525"></ListBox>
        
        <Button Grid.Row="1" Width="Auto" Height="30" HorizontalAlignment="Left" Name="btnDodajNamestaj" Click="btnDodajNamestaj_Click" Margin="22,20,0,14.8">Dodaj namestaj</Button>
        <Button Grid.Row="1" Width="Auto" Height="30" HorizontalAlignment="Right" Name="btnIzmeniNamestaj" Click="btnIzmeniNamestaj_Click" Margin="0,20,286.4,14.8">Izmeni namestaj</Button>
        
        <Button Grid.Row="1" Width="Auto" Height="30" HorizontalAlignment="Right" Name="btnIzlaz" Click="btnIzlaz_Click" Margin="0,17,10.4,17.8">Izlaz</Button>
    </Grid>
</Window>
