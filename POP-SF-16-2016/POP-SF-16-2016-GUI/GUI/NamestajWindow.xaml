<Window x:Class="POP_SF_16_2016_GUI.GUI.NamestajWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:POP_SF_16_2016_GUI.GUI"
        mc:Ignorable="d"
        Title="NamestajWindow" Height="190.099" Width="300">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="27*"/>
            <ColumnDefinition Width="119*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="44*"></RowDefinition>
            <RowDefinition Height="21*"></RowDefinition>
        </Grid.RowDefinitions>
        <Label Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="3">Naziv</Label>
        <TextBox Grid.Row="0" Grid.Column="1" Height="30" HorizontalAlignment="Left" Name="tbNaziv" VerticalAlignment="Top" Width="200"></TextBox>
        <Button Grid.Row="1" HorizontalAlignment="Right" Name="btnIzlaz" Click="btnIzlaz_Click" Grid.Column="1" >Zatvori</Button>
        <Button Grid.Row="1" HorizontalAlignment="Left" Name="btnSacuvaj" Click="btnSacuvaj_Click" Grid.Column="1" >Sacuvaj</Button>
    </Grid>


</Window>
