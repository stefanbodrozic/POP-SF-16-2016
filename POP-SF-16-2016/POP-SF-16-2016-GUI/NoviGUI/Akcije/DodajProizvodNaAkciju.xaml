<Window x:Class="POP_SF_16_2016_GUI.NoviGUI.Akcije.DodajProizvodNaAkciju"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:POP_SF_16_2016_GUI.NoviGUI.Akcije"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Title="Dodavanje proizvoda na akciju" Height="300" Width="450">
    <Grid>
        <Grid HorizontalAlignment="Left" Height="196" Margin="10,10,0,0" VerticalAlignment="Top" Width="422">
            <DataGrid x:Name="dgNamestaj" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Height="176" Width="402" AutoGeneratingColumn="dgNamestaj_AutoGeneratingColumn"/>
        </Grid>
        <Grid HorizontalAlignment="Left" Height="48" Margin="10,211,0,0" VerticalAlignment="Top" Width="422">
            <Button x:Name="btnDodaj" Content="Dodaj" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="80" Height="28" Click="btnDodaj_Click"/>
            <Button x:Name="btnIzlaz" Content="Izlaz" HorizontalAlignment="Left" Margin="332,10,0,0" VerticalAlignment="Top" Width="80" Height="28" Click="btnIzlaz_Click"/>
        </Grid>
    </Grid>
</Window>
